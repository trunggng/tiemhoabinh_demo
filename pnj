            </script><script id="tracking-utm" data-nscript="beforeInteractive">
              (function () {
  // Hàm lấy giá trị cookie dựa trên tên
  function getCookie(name) {
    try {
      const cookieArr = document.cookie.split(";"); // Tách từng cặp cookie
      for (let i = 0; i < cookieArr.length; i++) {
        const cookiePair = cookieArr[i].split("="); // Tách tên và giá trị
        if (name === cookiePair[0].trim()) {
          return decodeURIComponent(cookiePair[1]); // Trả về giá trị đã giải mã
        }
      }
      return null; // Trả về null nếu không tìm thấy
    } catch (error) {
      console.error("Error retrieving cookie:", error); // Bắt lỗi khi đọc cookie
      return null;
    }
  }

  // Hàm thiết lập cookie với các tham số: tên, giá trị, số ngày, và path
  function setCookie(name, value, days, path) {
    try {
      let expires = "";
      if (days) {
        const date = new Date();
        date.setTime(date.getTime() + days * 24 * 60 * 60 * 1000); // Thời hạn cookie
        expires = "; expires=" + date.toUTCString(); // Định dạng ngày hết hạn
      }
      document.cookie =
        name +
        "=" +
        encodeURIComponent(value) +
        expires +
        "; path=" +
        (path || "/") +
        "; Secure; SameSite=Lax"; // Thêm bảo mật cơ bản
    } catch (error) {
      console.error("Error setting cookie:", error); // Bắt lỗi khi thiết lập cookie
    }
  }

  // Lấy query parameters từ URL
  function getQueryParams() {
    try {
      const params = new URLSearchParams(window.location.search); // Lấy query string
      
      // Lấy aff_sid từ aff_sid hoặc click_id (cho CityAds affiliate)
      const aff_sid = params.get("aff_sid") || params.get("click_id") || "";
      
      return {
        utm_source: params.get("utm_source") || "",
        utm_medium: params.get("utm_medium") || "",
        utm_content: params.get("utm_content") || "",
        utm_campaign: params.get("utm_campaign") || "",
        aff_sid: aff_sid,
        transaction_id: params.get("transaction_id") || "",
      };
    } catch (error) {
      console.error("Error parsing query parameters:", error); // Bắt lỗi khi parse URL
      return {};
    }
  }

  // Main script logic
  try {
    const { utm_source, utm_medium, utm_content, utm_campaign, aff_sid, transaction_id } = getQueryParams();
    const utm_notifications = getCookie("utm_notifications");

    const newUtmNotifications = JSON.stringify({
      utm_source,
      utm_medium,
      utm_content,
      utm_campaign,
      aff_sid,
      transaction_id
    });

    // So sánh giá trị mới và giá trị hiện tại trong cookie
    if (
      (newUtmNotifications !== utm_notifications || !utm_notifications) &&
      utm_source // Đảm bảo utm_source tồn tại trước khi lưu cookie
    ) {
      setCookie("utm_notifications", newUtmNotifications, 30, "/");
      console.log("UTM notifications cookie updated.");
    } else {
      console.log("No changes detected in UTM notifications.");
    }
  } catch (error) {
    console.error("Error in script execution:", error); // Bắt lỗi chung
  }
})();

