                const isWebView = () => {
                  const userAgent = navigator.userAgent || navigator.vendor || window.opera;
                  const isIOSWebView = /iPhone|iPod|iPad/.test(userAgent) && !/Safari/.test(userAgent);
                  const isAndroidWebView =
                    /Android/.test(userAgent) &&
                    (/wv/.test(userAgent) || /Version\/\d+/.test(userAgent));
                  return isIOSWebView || isAndroidWebView;
                };
                
                const getWebViewType = () => {
                  const userAgent = navigator.userAgent || navigator.vendor || window.opera;
                  
                  if (/FBAN|FBAV|FB_IAB|Messenger/i.test(userAgent)) return 'facebook';
                  if (/Instagram/i.test(userAgent)) return 'instagram';
                  if (/TikTok/i.test(userAgent)) return 'tiktok';
                  if (/Line/i.test(userAgent)) return 'line';
                  if (/MicroMessenger/i.test(userAgent)) return 'wechat';
                  if (/Twitter/i.test(userAgent)) return 'twitter';
                  
                  return 'generic';
                };

                const redirectToDeviceBrowser = async () => {
                  if (!isWebView()) {
                    console.log("Not in a WebView, skipping redirection.");
                    return;
                  }

                  const url = window.location.href;
                  const userAgent = navigator.userAgent || navigator.vendor || window.opera;

                  if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
                    const webViewType = getWebViewType();
                    if (['facebook'].includes(webViewType)) {
                      return false;
                    } else {
                      const schemes = [
                        `x-safari-${url}`,
                        `com-apple-mobilesafari-tab:${url}`,
                        `googlechrome://${url.replace("https://", "").replace("http://", "")}`,
                        `firefox://open-url?url=${url}`,
                        `x-web-search://?${url}`
                      ];

                      for (const scheme of schemes) {
                        window.location.href = scheme;
                        await new Promise((r) => setTimeout(r, 100));
                      }
                      return false;
                    }
                  }

                  if (/android/i.test(userAgent)) {
                    const androidSchemes = [
                      `intent://${url.replace("https://", "")}#Intent;scheme=https;package=com.android.chrome;end;`,
                      `googlechrome://navigate?url=${url}`,
                      `firefox://open-url?url=${url}`
                    ];

                    for (const scheme of androidSchemes) {
                      window.location.href = scheme;
                      await new Promise((r) => setTimeout(r, 100));
                    }
                    return false;
                  }
                };

                // Thực thi ngay lập tức
                redirectToDeviceBrowser();
